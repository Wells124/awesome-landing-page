{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
	"exclude": ["node_modules", "dist", ".vercel", ".netlify", "public"]
}
